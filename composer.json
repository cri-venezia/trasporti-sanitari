{
	"name": "ahdcreative/cri_trasporti",
	"description": "Plugin per la gestione delle richieste di trasporto sanitario per Croce Rossa Italiana - Comitato di Venezia",
	"type": "wordpress-plugin",
	"license": "proprietary",
	"authors": [
		{
			"name": "Luca Forzutti",
			"email": "luca.forzutti@veneto.cri.it"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org",
			"only": [
				"wpackagist-plugin/*",
				"wpackagist-theme/*"
			]
		}
	],
	"require": {
		"php": ">=8.2",
		"tecnickcom/tcpdf": "^6.10",
		"yahnis-elsts/plugin-update-checker": "^5.6"
	},
	"require-dev": {
		"wpackagist-plugin/elementor": "*",
		"wpackagist-plugin/translatepress-multilingual": "*",
		"scssphp/scssphp": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"CRIVenice\\Transport\\": "src/"
		}
	},
	"scripts": {
		"compile-sass": "php bin/compile-scss.php"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true
		}
	},
	"extra": {
		"installer-paths": {
			"vendor/plugins/{$name}": [
				"type:wordpress-plugin"
			]
		}
	}
}
